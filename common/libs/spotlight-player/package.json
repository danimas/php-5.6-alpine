{
  "name": "@upshow/spotlight-player",
  "version": "2.0.2",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build:watch",
    "build": "babel src --out-dir lib --copy-files",
    "build:watch": "babel src --out-dir lib --copy-files --watch",
    "lint": "./node_modules/.bin/standardx",
    "lint-fix": "./node_modules/.bin/standardx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "camelcase": 0,
      "semi": [2, "always"],
      "indent": ["error", 4, {"ignoredNodes": ["JSXElement *"]}],
      "react/jsx-indent": ["error", 4],
      "react/jsx-closing-bracket-location": [0],
      "react/prop-types": [1, {
        "skipUndeclared": true
      }]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "~16.13.0",
    "react-image": "~2.4.0",
    "react-player": "~1.15.2",
    "@upshow/lottie": "~1.0.0",
    "@material-ui/icons": "~4.9.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.12.1",
    "@babel/plugin-proposal-optional-chaining": "~7.12.1",
    "luxon": "~1.23.0",
    "lodash": "~4.17.15",
    "react-circular-progressbar": "~2.0.3",
    "react-textfit": "~1.1.0",
    "kjua": "~0.9.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.8.4",
    "@babel/preset-react": "~7.8.3",
    "node-sass": "~4.13.1",
    "prop-types": "~15.7.2",
    "standardx": "~5.0.0"
  }
}
